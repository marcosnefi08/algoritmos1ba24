programa {
  funcao inicio() {

   //declaração de variáveis
   real vp, qp, total

   //entrada
   escreva("quanto custa o produto?: ")
   leia(vp)
   escreva("quantos deste produto você adquiriu?: ")
   leia(qp)

   //processamento
   total = vp*qp

   //saída
   escreva("o valor total a ser pago é: ", total)


  }
}
